<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qrcode">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.ScanBarCodeActivity"
            android:label="@string/scan_barcode" />
        <activity
            android:name=".activity.ScanBarCodeResultActivity"
            android:label="@string/scan_result" />
        <activity
            android:name=".activity.ScanGoodsBarCodeActivity"
            android:label="@string/scan_goods_barcode" />
        <activity
            android:name=".activity.GoodsInfoActivity"
            android:label="@string/goods_info" />
        <activity
            android:name=".activity.ScanQRCodeActivity"
            android:label="@string/scan_qrcode" />
        <activity
            android:name=".activity.ScanQRCodeResultActivity"
            android:label="@string/scan_result" />
        <activity
            android:name=".activity.ScanExpressActivity"
            android:label="@string/search"
            android:theme="@style/NoActionBarActivity" />
    </application>

</manifest>